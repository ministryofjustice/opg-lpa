{
    "name": "ministryofjustice/opg-make-logging",
    "description": "Logging module for service for creating Lasting Power of Attorney PDF documents",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/ministryofjustice/opg-lpa",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-component-installer": "^2.1.1",
        "laminas/laminas-mvc": "^3.1.1",
        "laminas/laminas-cache": "2.9.0",
        "laminas/laminas-mvc-i18n": "^1.1.1",
        "laminas/laminas-cli": "^1.1",
        "laminas/laminas-zendframework-bridge": "^1.1.1",
        "laminas/laminas-log": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "MakeLogger\\": "module/MakeLogger/src/"
        }
    }
}
