<phpunit bootstrap="./tests/Bootstrap.php" colors="true" strict="false" syntaxCheck="false" verbose="false">
    <testsuite name="OpgLpaPdf">
        <directory>./tests/OpgTest</directory>
    </testsuite>
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">./../src</directory>
        </whitelist>
    </filter>
    <php>
        <server name="SERVER_NAME" value="phpunit"/>
        <server name="APPLICATION_ENV" value="development"/>
    </php>
</phpunit>
