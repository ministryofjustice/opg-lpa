{
    "name": "ministryofjustice/opg-lpa-pdf",
    "description": "Worker service for creating Lasting Power of Attorney PDF documents",
    "minimum-stability": "stable",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels",
            "no-api": true
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.204.5",
        "mikehaertl/php-pdftk": "0.2.1",
        "mikehaertl/php-shellcommand": "1.6.4 as 1.0.2",
        "laminas/laminas-crypt": "^3.4.0",
        "laminas/laminas-filter": "^2.12.0",
        "ministryofjustice/opg-lpa-datamodels": "^14.0",
        "laminas/laminas-barcode": "^2.11",
        "tecnickcom/tcpdf": "6.4.1",
        "setasign/fpdi-tcpdf": "^2.0",
        "laminas/laminas-zendframework-bridge": "^1.1.1",
        "laminas/laminas-log": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4",
        "ext-json": "*",
        "vimeo/psalm": "^4.23"
    },
    "autoload": {
        "psr-4": {
            "Opg\\Lpa\\Pdf\\": "src/Opg/Lpa/Pdf/",
            "OpgTest\\Lpa\\Pdf\\": "tests/Opg/Lpa/Pdf/",
            "MakeLogger\\": "../shared/logging/module/MakeLogger/src/"
        }
    },
    "include-path": ["config/"]
}
