{
    "name": "ministryofjustice/opg-lpa-pdf",
    "description": "Worker service for creating Lasting Power of Attorney PDF documents",
    "minimum-stability": "stable",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-logger",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "^3.91",
        "mikehaertl/php-pdftk": "0.2.1",
        "zendframework/zendpdf": "^2.0.2",
        "laminas/laminas-crypt": "^3.3.1",
        "laminas/laminas-filter": "^2.9.4",
        "ministryofjustice/opg-lpa-datamodels": "^13.2",
        "ministryofjustice/opg-lpa-logger": "^5.0",
        "laminas/laminas-barcode": "^2.5",
        "tecnickcom/tcpdf": "6.4.1",
        "setasign/fpdi-tcpdf": "^2.0",
        "laminas/laminas-zendframework-bridge": "^1.1.1",
        "laminas/laminas-log": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {"Opg\\Lpa\\Pdf\\": "src/Opg/Lpa/Pdf/", "OpgTest\\Lpa\\Pdf\\": "tests/Opg/Lpa/Pdf/"}
    },
    "include-path": ["config/"]
}
