{
    "name": "ministryofjustice/opg-lpa-pdf",
    "description": "Worker service for creating Lasting Power of Attorney PDF documents",
    "minimum-stability": "stable",
    "license": "MIT",
    "repositories": [
    ],
    "require": {
        "php": "8.4.*",
        "aws/aws-sdk-php": "^3.204.5",
        "mikehaertl/php-pdftk": "0.13.1",
        "mikehaertl/php-shellcommand": "1.6.4 as 1.0.2",
        "laminas/laminas-filter": "^2.12.0",
        "laminas/laminas-barcode": "^2.15",
        "tecnickcom/tcpdf": "6.8.0",
        "laminas/laminas-zendframework-bridge": "^1.1.1",
        "monolog/monolog": "^3.9",
        "symfony/validator": "^7.0",
        "egulias/email-validator": "^4",
        "setasign/fpdi": "2.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4",
        "ext-json": "*",
        "ext-imagick": "*",
        "vimeo/psalm": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Opg\\Lpa\\Pdf\\": "src/Opg/Lpa/Pdf/",
            "OpgTest\\Lpa\\Pdf\\": "tests/Opg/Lpa/Pdf/",
            "MakeShared\\": "../shared/module/MakeShared/src/",
            "MakeSharedTest\\": "../shared/module/MakeShared/tests/"
        }
    },
    "include-path": ["config/"],
    "config": {
        "platform": {
            "php": "8.4",
            "ext-imagick": "1"
        }
    }
}
